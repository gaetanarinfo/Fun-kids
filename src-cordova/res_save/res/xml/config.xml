<?xml version='1.0' encoding='utf-8'?>
<widget id="org.funkids.app" version="1.0.29"
    xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CDVOrientation">
        <param name="android-package" value="cordova.plugins.screenorientation.CDVOrientation" />
    </feature>
    <feature name="InAppBillingPlugin">
        <param name="android-package" value="cc.fovea.PurchasePlugin" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="CordovaHttpPlugin">
        <param name="android-package" value="com.silkimen.cordovahttp.CordovaHttpPlugin" />
    </feature>
    <feature name="WonderPushPlugin">
        <param name="android-package" value="com.wonderpush.sdk.cordova.WonderPushPlugin" />
    </feature>
    <feature name="PlayGamesServices">
        <param name="android-package" value="com.berriart.cordova.plugins.PlayGamesServices" />
    </feature>
    <name>Funkids</name>
    <description>Funkids - Jeu de RÃ©flexion gratuit</description>
    <author email="support@funkids.fr" href="https://cordova.apache.org">
        Funkids Support
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="about:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="android-targetSdkVersion" value="31" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="android-minSdkVersion" value="21" />
</widget>
